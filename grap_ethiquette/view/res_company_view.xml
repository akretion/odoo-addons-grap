<openerp>
    <data>
        <record id="view_res_company_form_base" model="ir.ui.view" >
            <field name="name">res.company.form.base</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group[@string='Report Footer Configuration']" position="after">
                    <group string="Grap Ethiquette">
                        <field widget="image" name="ethiquette_image"/>
                        <field name="ethiquette_color"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</openerp>

