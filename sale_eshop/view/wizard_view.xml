<openerp>
    <data>

<!-- Define: wizard.change.product.category -->
        <record id="view_change_product_category" model="ir.ui.view">
            <field name="model">wizard.change.product.category</field>
            <field name="arch" type="xml">
                <form string="Move to other eShop Category" version="7.0">
                    <group>
                        <field name="old_eshop_category_id"/>
                        <field name="new_eshop_category_id"/>
                    </group>
                    <footer>
                        <button name="change_product_category" string="_Apply" type="object" class="oe_highlight"/>
                        <label string="or"/>
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

    </data>
</openerp>
